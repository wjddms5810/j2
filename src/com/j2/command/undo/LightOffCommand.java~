package com.j2.command.undo;

public class LightOffCommand implements Command{
  Light light;
  int level;
  public LightOffCommand(){}
  public LightOffCommand(Light light){
    this.light = light;
  }
  public void execute(){
    level = light.getLevel();
    light.on();
  }
  public void undo(){
    light.dim(level);
  }
}